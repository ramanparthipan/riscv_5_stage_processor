#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5c5b8b531f40 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5c5b8b4f6cf0 .scope module, "tb_program_counter" "tb_program_counter" 3 3;
 .timescale -9 -12;
P_0x5c5b8b51c5a0 .param/l "ADDR_WIDTH" 1 3 4, +C4<00000000000000000000000000100000>;
P_0x5c5b8b51c5e0 .param/l "CLK_PERIOD" 1 3 5, +C4<00000000000000000000000000001010>;
v0x5c5b8b5218b0_0 .var "clk", 0 0;
v0x5c5b8b522560_0 .var "en", 0 0;
v0x5c5b8b54a340_0 .net "pc_out", 31 0, L_0x5c5b8b5204f0;  1 drivers
v0x5c5b8b54a440_0 .var "rst_n", 0 0;
E_0x5c5b8b5308b0 .event posedge, v0x5c5b8b520430_0;
S_0x5c5b8b4f6f20 .scope module, "dut" "program_counter" 3 16, 4 4 0, S_0x5c5b8b4f6cf0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /OUTPUT 32 "pc_out";
P_0x5c5b8b4f7100 .param/l "ADDR_WIDTH" 0 4 5, +C4<00000000000000000000000000100000>;
L_0x5c5b8b5204f0 .functor BUFZ 32, v0x5c5b8b520a40_0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x5c5b8b520430_0 .net "clk", 0 0, v0x5c5b8b5218b0_0;  1 drivers
v0x5c5b8b520610_0 .net "en", 0 0, v0x5c5b8b522560_0;  1 drivers
v0x5c5b8b520810_0 .net "pc_out", 31 0, L_0x5c5b8b5204f0;  alias, 1 drivers
v0x5c5b8b520a40_0 .var "pc_reg", 31 0;
v0x5c5b8b5216d0_0 .net "rst_n", 0 0, v0x5c5b8b54a440_0;  1 drivers
E_0x5c5b8b4f76a0/0 .event negedge, v0x5c5b8b5216d0_0;
E_0x5c5b8b4f76a0/1 .event posedge, v0x5c5b8b520430_0;
E_0x5c5b8b4f76a0 .event/or E_0x5c5b8b4f76a0/0, E_0x5c5b8b4f76a0/1;
    .scope S_0x5c5b8b4f6f20;
T_0 ;
    %wait E_0x5c5b8b4f76a0;
    %load/vec4 v0x5c5b8b5216d0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5c5b8b520a40_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5c5b8b520610_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5c5b8b520a40_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x5c5b8b520a40_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5c5b8b4f6cf0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c5b8b5218b0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5c5b8b5218b0_0;
    %inv;
    %store/vec4 v0x5c5b8b5218b0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5c5b8b4f6cf0;
T_2 ;
    %vpi_call/w 3 31 "$monitor", "Time: %0t | Reset: %b | Enable: %b | PC_Out: %h (%0d)", $time, v0x5c5b8b54a440_0, v0x5c5b8b522560_0, v0x5c5b8b54a340_0, v0x5c5b8b54a340_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5c5b8b4f6cf0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c5b8b54a440_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c5b8b522560_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c5b8b54a440_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c5b8b522560_0, 0, 1;
    %wait E_0x5c5b8b5308b0;
    %wait E_0x5c5b8b5308b0;
    %wait E_0x5c5b8b5308b0;
    %wait E_0x5c5b8b5308b0;
    %delay 2000, 0;
    %vpi_call/w 3 51 "$display", "--- Disabling PC increment ---" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c5b8b522560_0, 0, 1;
    %wait E_0x5c5b8b5308b0;
    %wait E_0x5c5b8b5308b0;
    %delay 2000, 0;
    %vpi_call/w 3 57 "$display", "--- Re-enabling PC increment ---" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c5b8b522560_0, 0, 1;
    %wait E_0x5c5b8b5308b0;
    %wait E_0x5c5b8b5308b0;
    %vpi_call/w 3 62 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "verif/src/program_counter_tb.sv";
    "design/rtl/program_counter.sv";
