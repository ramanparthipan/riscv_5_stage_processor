#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x59682b54fd30 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x59682b54fec0 .scope module, "tb_program_counter" "tb_program_counter" 3 3;
 .timescale -9 -12;
P_0x59682b539280 .param/l "ADDR_WIDTH" 1 3 4, +C4<00000000000000000000000000100000>;
P_0x59682b5392c0 .param/l "CLK_PERIOD" 1 3 5, +C4<00000000000000000000000000001010>;
v0x59682b569250_0 .var "alu_out", 31 0;
v0x59682b569330_0 .var "clk", 0 0;
v0x59682b5693d0_0 .var "en", 0 0;
v0x59682b5694d0_0 .var "jump_en", 0 0;
v0x59682b5695a0_0 .net "pc_out", 31 0, L_0x59682b53c420;  1 drivers
v0x59682b569690_0 .var "rst_n", 0 0;
E_0x59682b5146a0 .event posedge, v0x59682b53ef20_0;
S_0x59682b513cf0 .scope module, "dut" "program_counter" 3 18, 4 3 0, S_0x59682b54fec0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /INPUT 1 "jump_en";
    .port_info 4 /INPUT 32 "alu_out";
    .port_info 5 /OUTPUT 32 "pc_out";
P_0x59682b513ed0 .param/l "ADDR_WIDTH" 0 4 4, +C4<00000000000000000000000000100000>;
L_0x59682b53c420 .functor BUFZ 32, v0x59682b568ff0_0, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x59682b53e490_0 .net "alu_out", 31 0, v0x59682b569250_0;  1 drivers
v0x59682b53ef20_0 .net "clk", 0 0, v0x59682b569330_0;  1 drivers
v0x59682b53f440_0 .net "en", 0 0, v0x59682b5693d0_0;  1 drivers
v0x59682b568e00_0 .net "jump_en", 0 0, v0x59682b5694d0_0;  1 drivers
v0x59682b568ec0_0 .net "pc_out", 31 0, L_0x59682b53c420;  alias, 1 drivers
v0x59682b568ff0_0 .var "pc_reg", 31 0;
v0x59682b5690d0_0 .net "rst_n", 0 0, v0x59682b569690_0;  1 drivers
E_0x59682b537f30/0 .event negedge, v0x59682b5690d0_0;
E_0x59682b537f30/1 .event posedge, v0x59682b53ef20_0;
E_0x59682b537f30 .event/or E_0x59682b537f30/0, E_0x59682b537f30/1;
    .scope S_0x59682b513cf0;
T_0 ;
    %wait E_0x59682b537f30;
    %load/vec4 v0x59682b5690d0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x59682b568ff0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x59682b568e00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x59682b53e490_0;
    %assign/vec4 v0x59682b568ff0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x59682b53f440_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x59682b568ff0_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x59682b568ff0_0, 0;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x59682b54fec0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59682b569330_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x59682b569330_0;
    %inv;
    %store/vec4 v0x59682b569330_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x59682b54fec0;
T_2 ;
    %vpi_call/w 3 35 "$monitor", "Time: %0t | Rst: %b | En: %b | Jmp: %b | ALU_Out: %h | PC_Out: %h", $time, v0x59682b569690_0, v0x59682b5693d0_0, v0x59682b5694d0_0, v0x59682b569250_0, v0x59682b5695a0_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x59682b54fec0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59682b569690_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59682b5693d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59682b5694d0_0, 0, 1;
    %pushi/vec4 4294967295, 4294967295, 32;
    %store/vec4 v0x59682b569250_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x59682b569690_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x59682b5693d0_0, 0, 1;
    %wait E_0x59682b5146a0;
    %wait E_0x59682b5146a0;
    %delay 2000, 0;
    %vpi_call/w 3 52 "$display", "--- Testing Jump ---" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x59682b5694d0_0, 0, 1;
    %pushi/vec4 3405691582, 0, 32;
    %store/vec4 v0x59682b569250_0, 0, 32;
    %wait E_0x59682b5146a0;
    %delay 2000, 0;
    %vpi_call/w 3 59 "$display", "--- Resuming Increment from Jump Address ---" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59682b5694d0_0, 0, 1;
    %pushi/vec4 4294967295, 4294967295, 32;
    %store/vec4 v0x59682b569250_0, 0, 32;
    %wait E_0x59682b5146a0;
    %wait E_0x59682b5146a0;
    %vpi_call/w 3 65 "$finish" {0 0 0};
    %end;
    .thread T_3;
