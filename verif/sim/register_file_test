E_0x60306f7192d0/0 .event negedge, v0x60306f740e90_0;
E_0x60306f7192d0/1 .event posedge, v0x60306f740990_0;
E_0x60306f7192d0 .event/or E_0x60306f7192d0/0, E_0x60306f7192d0/1;
L_0x60306f7419d0 .cmp/eq 5, v0x60306f7413b0_0, v0x60306f741900_0;
L_0x60306f741b90 .array/port v0x60306f740dd0, L_0x60306f741c30;
L_0x60306f741c30 .concat [ 5 2 0 0], v0x60306f7413b0_0, L_0x71942a536018;
L_0x60306f741d70 .functor MUXZ 32, L_0x60306f741b90, v0x60306f741760_0, L_0x60306f70beb0, C4<>;
L_0x60306f741f80 .cmp/eq 5, v0x60306f741450_0, v0x60306f741900_0;
L_0x60306f7420c0 .array/port v0x60306f740dd0, L_0x60306f7421a0;
L_0x60306f7421a0 .concat [ 5 2 0 0], v0x60306f741450_0, L_0x71942a536060;
