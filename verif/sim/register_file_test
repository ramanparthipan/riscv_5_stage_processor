S_0x60306f71d0e0 .scope begin, "$unm_blk_2" "$unm_blk_2" 4 32, 4 32 0, S_0x60306f71ce00;
 .timescale 0 0;
v0x60306f70bfd0_0 .var/i "i", 31 0;
    .scope S_0x60306f71ce00;
T_0 ;
    %wait E_0x60306f7192d0;
    %load/vec4 v0x60306f740e90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %fork t_1, S_0x60306f71d0e0;
    %jmp t_0;
    .scope S_0x60306f71d0e0;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x60306f70bfd0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x60306f70bfd0_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_0.3, 5;
    %pushi/vec4 0, 0, 32;
    %ix/getv/s 3, v0x60306f70bfd0_0;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x60306f740dd0, 0, 4;
    %load/vec4 v0x60306f70bfd0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x60306f70bfd0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %end;
    .scope S_0x60306f71ce00;
t_0 %join;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x60306f741030_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x60306f7410f0_0;
    %cmpi/ne 0, 0, 5;
    %jmp/0xz  T_0.6, 4;
    %load/vec4 v0x60306f740f50_0;
    %load/vec4 v0x60306f7410f0_0;
    %pad/u 7;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x60306f740dd0, 0, 4;
T_0.6 ;
T_0.4 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x60306f712f40;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60306f7412f0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x60306f7412f0_0;
    %inv;
    %store/vec4 v0x60306f7412f0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x60306f712f40;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60306f741690_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60306f741690_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x60306f712f40;
T_3 ;
    %wait E_0x60306f718e60;
    %wait E_0x60306f719740;
    %vpi_call/w 3 39 "$display", "--- Testing Normal Write/Read Operation ---" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60306f741830_0, 0, 1;
    %pushi/vec4 5, 0, 5;
    %store/vec4 v0x60306f741900_0, 0, 5;
    %pushi/vec4 2863311530, 0, 32;
    %store/vec4 v0x60306f741760_0, 0, 32;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x60306f7413b0_0, 0, 5;
    %vpi_call/w 3 45 "$display", "Time:%0t | Writing 0x%h to x5.", $time, v0x60306f741760_0 {0 0 0};
    %wait E_0x60306f719740;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60306f741830_0, 0, 1;
    %pushi/vec4 5, 0, 5;
    %store/vec4 v0x60306f7413b0_0, 0, 5;
    %delay 1000, 0;
    %vpi_call/w 3 52 "$display", "Time:%0t | Reading from x5.", $time {0 0 0};
    %load/vec4 v0x60306f741520_0;
    %cmpi/e 2863311530, 0, 32;
    %jmp/0xz  T_3.0, 4;
    %vpi_call/w 3 54 "$display", "PASS: Normal read-back of 0x%h was successful.", v0x60306f741520_0 {0 0 0};
    %jmp T_3.1;
T_3.0 ;
    %vpi_call/w 3 56 "$error", "FAIL: Normal read-back was incorrect. Got 0x%h", v0x60306f741520_0 {0 0 0};
T_3.1 ;
    %wait E_0x60306f719740;
    %vpi_call/w 3 64 "$display", "\012--- Testing Internal Forwarding ---" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60306f741830_0, 0, 1;
    %pushi/vec4 10, 0, 5;
    %store/vec4 v0x60306f741900_0, 0, 5;
    %pushi/vec4 3735928559, 0, 32;
    %store/vec4 v0x60306f741760_0, 0, 32;
    %pushi/vec4 10, 0, 5;
    %store/vec4 v0x60306f7413b0_0, 0, 5;
    %vpi_call/w 3 69 "$display", "Time:%0t | Simultaneously writing 0x%h to x10 and reading from x10.", $time, v0x60306f741760_0 {0 0 0};
    %delay 1000, 0;
    %load/vec4 v0x60306f741520_0;
    %cmpi/e 3735928559, 0, 32;
    %jmp/0xz  T_3.2, 4;
    %vpi_call/w 3 74 "$display", "PASS: Forwarding successful. Read 0x%h before clock edge.", v0x60306f741520_0 {0 0 0};
    %jmp T_3.3;
T_3.2 ;
    %vpi_call/w 3 76 "$error", "FAIL: Forwarding was incorrect. Got 0x%h", v0x60306f741520_0 {0 0 0};
T_3.3 ;
    %wait E_0x60306f719740;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60306f741830_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x60306f741520_0;
    %cmpi/e 3735928559, 0, 32;
    %jmp/0xz  T_3.4, 4;
    %vpi_call/w 3 85 "$display", "PASS: Value 0x%h was correctly written to storage.", v0x60306f741520_0 {0 0 0};
    %jmp T_3.5;
T_3.4 ;
    %vpi_call/w 3 87 "$error", "FAIL: Value was not written correctly to storage." {0 0 0};
T_3.5 ;
    %vpi_call/w 3 90 "$finish" {0 0 0};
    %end;
    .thread T_3;
