    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 10 "addr";
    .port_info 2 /OUTPUT 32 "data";
P_0x61f0bc43e4b0 .param/l "ADDR_BITS" 0 4 4, +C4<00000000000000000000000000001010>;
P_0x61f0bc43e4f0 .param/l "DATA_BITS" 0 4 5, +C4<00000000000000000000000000100000>;
v0x61f0bc418ff0_0 .net "addr", 9 0, v0x61f0bc468fb0_0;  1 drivers
v0x61f0bc468b20_0 .net "clk", 0 0, v0x61f0bc468e10_0;  1 drivers
v0x61f0bc468be0_0 .var "data", 31 0;
v0x61f0bc468cd0 .array "mem", 1023 0, 31 0;
E_0x61f0bc452c90 .event posedge, v0x61f0bc468b20_0;
    .scope S_0x61f0bc418cf0;
T_0 ;
    %wait E_0x61f0bc452c90;
    %load/vec4 v0x61f0bc418ff0_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x61f0bc468cd0, 4;
    %assign/vec4 v0x61f0bc468be0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x61f0bc453750;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61f0bc468e10_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x61f0bc468e10_0;
    %inv;
    %store/vec4 v0x61f0bc468e10_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x61f0bc453750;
T_2 ;
    %vpi_call/w 3 28 "$readmemh", "verif/src/program.hex", v0x61f0bc468cd0 {0 0 0};
    %pushi/vec4 0, 0, 10;
    %store/vec4 v0x61f0bc468fb0_0, 0, 10;
    %delay 20000, 0;
    %vpi_func/s 3 32 "$sformatf", "Retrieved instruction, %h", v0x61f0bc468ee0_0 {0 0 0};
    %vpi_call/w 3 32 "$display", S<0,str> {0 0 1};
    %pushi/vec4 1, 0, 10;
    %store/vec4 v0x61f0bc468fb0_0, 0, 10;
    %delay 20000, 0;
    %vpi_func/s 3 35 "$sformatf", "Retrieved instruction, %h", v0x61f0bc468ee0_0 {0 0 0};
    %vpi_call/w 3 35 "$display", S<0,str> {0 0 1};
    %pushi/vec4 2, 0, 10;
    %store/vec4 v0x61f0bc468fb0_0, 0, 10;
    %delay 20000, 0;
    %vpi_func/s 3 38 "$sformatf", "Retrieved instruction, %h", v0x61f0bc468ee0_0 {0 0 0};
