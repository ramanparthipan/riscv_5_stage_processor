    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x646d74f96320_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x646d74f95730_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x646d74f95f00_0;
    %assign/vec4 v0x646d74f95fc0_0, 0;
