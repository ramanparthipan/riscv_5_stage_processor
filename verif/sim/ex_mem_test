    %assign/vec4 v0x646d74f96160_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x646d74f95c80_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x646d74f95920_0, 0;
