    %load/vec4 v0x5c835e1a5f70_0;
    %inv;
    %store/vec4 v0x5c835e1a5f70_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5c835e190aa0;
T_2 ;
