    %load/vec4 v0x5c835e156010_0;
    %pad/u 33;
    %addi 1, 0, 33;
    %ix/vec4 4;
    %load/vec4a v0x5c835e1a5d40, 4;
    %concat/vec4; draw_concat_vec4
    %ix/getv 4, v0x5c835e156010_0;
    %load/vec4a v0x5c835e1a5d40, 4;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5c835e1a5c50_0, 0;
    %jmp T_0;
