TD_branch_tb.check_branch ;
    %load/vec4 v0x5b1692540ee0_0;
    %store/vec4 v0x5b1692541940_0, 0, 3;
    %load/vec4 v0x5b169251ec50_0;
    %store/vec4 v0x5b1692541bd0_0, 0, 32;
    %load/vec4 v0x5b16924ce7b0_0;
    %store/vec4 v0x5b1692541cc0_0, 0, 32;
    %delay 1000, 0;
    %load/vec4 v0x5b1692541db0_0;
    %load/vec4 v0x5b16924ceba0_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x5b1692540ee0_0;
    %store/vec4 v0x5b1692541280_0, 0, 3;
    %callf/str TD_branch_tb.comp_op_to_string, S_0x5b1692540fc0;
    %vpi_call/w 3 36 "$display", "PASS: %s(%d, %d) => %b", S<0,str>, v0x5b169251ec50_0, v0x5b16924ce7b0_0, v0x5b1692541db0_0 {0 0 1};
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5b1692540ee0_0;
    %store/vec4 v0x5b1692541280_0, 0, 3;
    %callf/str TD_branch_tb.comp_op_to_string, S_0x5b1692540fc0;
    %vpi_call/w 3 38 "$error", "FAIL: %s(%d, %d) => Got %b, Expected %b", S<0,str>, v0x5b169251ec50_0, v0x5b16924ce7b0_0, v0x5b1692541db0_0, v0x5b16924ceba0_0 {0 0 1};
T_0.1 ;
    %end;
S_0x5b1692540fc0 .scope function.str, "comp_op_to_string" "comp_op_to_string" 3 19, 3 19 0, S_0x5b169250b420;
 .timescale -9 -12;
; Variable comp_op_to_string is string return value of scope S_0x5b1692540fc0
v0x5b1692541280_0 .var "op", 2 0;
TD_branch_tb.comp_op_to_string ;
    %load/vec4 v0x5b1692541280_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_1.5, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_1.6, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 3;
    %cmp/u;
    %jmp/1 T_1.7, 6;
    %pushi/str "BR_NOP";
    %ret/str 0; Assign to comp_op_to_string
    %disable S_0x5b1692540fc0;
    %jmp T_1.9;
T_1.2 ;
    %pushi/str "BR_EQ";
    %ret/str 0; Assign to comp_op_to_string
    %disable S_0x5b1692540fc0;
    %jmp T_1.9;
T_1.3 ;
    %pushi/str "BR_NE";
    %ret/str 0; Assign to comp_op_to_string
    %disable S_0x5b1692540fc0;
    %jmp T_1.9;
T_1.4 ;
    %pushi/str "BR_LT";
    %ret/str 0; Assign to comp_op_to_string
    %disable S_0x5b1692540fc0;
    %jmp T_1.9;
T_1.5 ;
    %pushi/str "BR_GE";
    %ret/str 0; Assign to comp_op_to_string
    %disable S_0x5b1692540fc0;
    %jmp T_1.9;
T_1.6 ;
    %pushi/str "BR_LTU";
    %ret/str 0; Assign to comp_op_to_string
    %disable S_0x5b1692540fc0;
    %jmp T_1.9;
T_1.7 ;
    %pushi/str "BR_GEU";
    %ret/str 0; Assign to comp_op_to_string
    %disable S_0x5b1692540fc0;
    %jmp T_1.9;
T_1.9 ;
    %pop/vec4 1;
    %end;
